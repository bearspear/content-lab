{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "yarn"
  },
  "newProjectRoot": "apps",
  "projects": {
    "content-lab": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/content-lab",
      "sourceRoot": "apps/content-lab/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/apps/content-lab",
            "index": "apps/content-lab/src/index.html",
            "browser": "apps/content-lab/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/content-lab/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/content-lab/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor",
                "output": "assets/monaco-editor"
              }
            ],
            "styles": [
              "apps/content-lab/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "html2pdf.js"
            ],
            "loader": {
              ".ttf": "file"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "50kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 5001
          },
          "configurations": {
            "production": {
              "buildTarget": "content-lab:build:production"
            },
            "development": {
              "buildTarget": "content-lab:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "content-lab:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "apps/content-lab/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/content-lab/src/favicon.ico",
              "apps/content-lab/src/assets"
            ],
            "styles": [
              "apps/content-lab/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
